{
  "name": "@qualweb/wcag-techniques",
  "version": "0.1.12",
  "description": "Implementation of the WCAG 2.1 techniques",
  "main": "dist/wcag.js",
  "types": "./dist",
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint src/**/*",
    "test": "mocha --require esm test/**/*",
